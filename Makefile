SRCS			= 	src/ft_strlen.s
OBJS			= 	$(SRCS:.s=.o)
NAME			=	libasm.a

.s.o:
		nasm $(<:.c=.o)

all:	${OBJS}
		ar rcs ${NAME} ${OBJS}

clean:

fclean:

re: